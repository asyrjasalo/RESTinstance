{
  "status": {
    "type": "integer",
    "enum": [
        200,
        201,
        204
    ]
  },
  "body": {
    "type": ["string", "array", "object"],
    "properties": {
      "id": {
        "type": "integer",
        "minimum": 1
      },
      "name": {
        "type": "string",
        "minLength": 2
      },
      "username": {
        "type": "string",
        "pattern": "[a-z]+"
      },
      "email": {
        "type": "string",
        "format": "email"
      },
      "address": {
        "type": "object",
        "properties": {
          "street": {
            "type": "string"
          },
          "suite": {
            "type": "string"
          },
          "city": {
            "type": "string"
          },
          "zipcode": {
            "type": "string"
          },
          "geo": {
            "type": "object",
            "properties": {
              "lat": {
                "type": "string"
              },
              "lng": {
                "type": "string"
              }
            },
            "required": [
              "lat",
              "lng"
            ]
          }
        },
        "required": [
          "city"
        ]
      },
      "phone": {
        "type": "string"
      },
      "website": {
        "type": "string"
      },
      "company": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string"
          },
          "catchPhrase": {
            "type": "string"
          },
          "bs": {
            "type": "string"
          }
        },
        "required": [
          "bs",
          "catchPhrase",
          "name"
        ]
      }
    },
    "required": [
      "address",
      "company",
      "email",
      "id",
      "name",
      "phone",
      "username",
      "website"
    ]
  }
}
